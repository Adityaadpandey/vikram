{
	"compilerOptions": {
	  "target": "ESNext",
	  "module": "NodeNext",
	  "moduleResolution": "NodeNext",
	  "declaration": true,
	  "outDir": "./dist",
	  "strict": true,
	  "esModuleInterop": true,
	  "allowSyntheticDefaultImports": true,
	  "skipLibCheck": true,
	  "forceConsistentCasingInFileNames": true
	},
	"include": ["./client.ts", "./prisma/**/*"],
	"exclude": ["node_modules", "dist", "generated"]
  }
